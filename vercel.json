{
  "headers": [
    {
      "source": "/(.*)\\.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!sitemap\\.xml|robots\\.txt|sitemaps\\/.*\\.xml).*)",
      "destination": "/index.html"
    }
  ]
}